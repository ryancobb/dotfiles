{
  "json_log": {
    "title": "CustomersDot log",
    "description": "Log format used by CustomersDot",
    "json": true,
    "file-pattern": ".*_json.log$",
    "level-field": "severity",
    "timestamp-field": "time",
    "value": {
      "message": {
        "kind": "string"
      },
      "params": {
        "kind": "json"
      },
      "backtrace#": {
        "kind": "json"
      }
    },  
    "line-format": [
      {
        "field": "time"
      },
      "|",
      {
        "field": "severity"
      },
      "|",
      {
        "field": "message"
      }
    ]
  }
}
